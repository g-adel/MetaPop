{"editor.wordWrap": "on",
"files.exclude": {
    "**/*.aux": true,
    "**/*.log": true,
    "**/*.out": true,
    "**/*.synctex.gz": true,
    "**/*.bbl": true,
    "**/*.blg": true,
    "**/*.fdb_latexmk": true,
    "**/*.fls": true,
    "**/*.toc": true,
},
"[latex]": {
    "editor.wordBasedSuggestions": "off",
},
"latex-workshop.message.badbox.show": "none",
"cSpell.words": [
    "governorates",
    "infecteds",
    "linearization",
    "Markovian",
    "metapopulation",
    "metapopulations",
    "socio",
    "susceptibles"
],
"spellright.language": [
    "en"
],
"spellright.documentTypes": [
    "markdown",
    "plaintext"
],
}